[user]
	name = twealm
	email = twealm@gmail.com

[color]
	    diff = auto
	    status = auto
	    branch = auto
	    interactive = auto
		ui = auto
[color "branch"]
	  current = yellow reverse
	  local = yellow
	  remote = green

[color "diff"]
	  meta = yellow bold
	  frag = magenta
	  plain = white bold
	  old = red bold
	  new = green bold
	  commit = yellow bold
	  func = green dim
	  #plain = white bold

[color "status"]
      added = yellow
	  changed = green
	  untracked = cyan

[credential]
	helper = osxkeychain

[core]
	editor = /usr/bin/vim

[alias]
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	st = status
	di = diff
	cam = commit -a -m
	pom = push origin master
